{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "akaunting/laravel-firewall": "^2.3",
        "botman/botman": "^2.8",
        "botman/driver-web": "^1.5",
        "danielme85/laravel-log-to-db": "^4.1",
        "hansschouten/phpagebuilder": "^0.31.0",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image-laravel": "^1.3",
        "laravel/fortify": "^1.24",
        "laravel/framework": "^11.9",
        "laravel/pulse": "^1.4",
        "laravel/telescope": "^5.9",
        "laravel/tinker": "^2.9",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^2.0",
        "monishroy/visitor-tracking": "^1.1",
        "nwidart/laravel-modules": "^11.1",
        "predis/predis": "2.0",
        "spatie/laravel-data": "^4.11",
        "spatie/laravel-permission": "^6.10",
        "spatie/laravel-translatable": "^6.9",
        "stichoza/google-translate-php": "^5.2",
        "tightenco/ziggy": "^2.4",
        "unisharp/laravel-filemanager": "^2.12"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ]
        }
    },
    "config": {
        "block-insecure": false,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
